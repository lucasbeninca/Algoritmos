algoritmo "semnome"
var
   idade, anoAtual, anoNascimento : inteiro


inicio

      escreval("escreva o ano atual")
      leia(anoAtual)
      escreval("escreva o ano que você nasceu")
      leia (anoNascimento)

      idade <- (anoAtual - anoNascimento)

      escreval("você tem : ",idade," anos")

fimalgoritmo